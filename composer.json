{
    "name": "bogddan/Bogddan",
    "description": "A Laravel package to easily generate CSV files from Eloquent model.",
    "homepage": "https://github.com/usmanhalalit/Bogddan",
    "keywords": ["laravel", "csv", "eloquent", "export"],
    "license": "MIT",
    "authors":
    [
        {
            "name": "Muhammad Usman",
            "email": "hi@usman.it",
            "role": "Developer"
        }
    ],

    "require": {
        "php": ">=7.1",
        "illuminate/database": ">=5.2",
        "league/csv": "^9.0"
    },

    "require-dev": {
        "fzaninotto/faker": "^1.8",
        "phpunit/phpunit": "^7.0"
    },

    "autoload":	{
        "psr-4": {
            "Bogddan\\": "src/Bogddan/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Bogddan\\": "tests/Bogddan/"
        },

        "classmap": [
            "tests/TestCase.php"
        ]
    }
}
